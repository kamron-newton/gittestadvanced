        <con:assertion type="JsonPath Match" name="Check EX Transaction for negation of 250 points + repurchase of 250 points + award of 250 points" id="c93894c5-eaab-4d2d-a1a2-44f9371f16b6">
          <con:configuration>
            <path>$.[?(@.TransactionId == '${EX Transaction#RawTransactionId}' &amp;&amp; @.MomentId == '${Upsert Fixed 250 Point Moment with 500 Cap#MomentId}')].PointsAmount</path>
            <content>[
   250.0,
   250.0,
   -250.0
]</content>
            <allowWildcards>false</allowWildcards>
            <ignoreNamspaceDifferences>false</ignoreNamspaceDifferences>
            <ignoreComments>false</ignoreComments>
