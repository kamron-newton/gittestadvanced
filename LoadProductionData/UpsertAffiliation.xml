      <script>import static com.jayway.jsonpath.JsonPath.parse

//Get the parsed json of GetSources test step response
def json = new groovy.json.JsonSlurper().parseText(context.expand('${GetAffiliations#Response}'))

//datasource
def AffiliationsDatasource =context.expand( '${Affiliations#AffiliationCode}')

def getAffIdByAffCode = { code ->  json.find {it?.AffiliationCode == AffiliationsDatasource}?.AffiliationId }

def AffiliationId= (getAffIdByAffCode('AffiliationsDatasource'))

if ( AffiliationId == "" || AffiliationId== null ) 
    testRunner.gotoStepByName("AddAffiliation")
else{
	testRunner.gotoStepByName("AffiliationId")
}</script>
