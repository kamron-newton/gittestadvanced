    <con:settings/>
    <con:config>
      <script>import static com.jayway.jsonpath.JsonPath.parse

//Get the parsed json of GetSources test step response
def json = new groovy.json.JsonSlurper().parseText(context.expand('${GetCountries#Response}'))

//datasource
def CountriesDatasource =context.expand( '${Countries#CountryCode}')

//CountryId
def GetCountryIdByCountryCode = { code ->  json.find {it?.CountryCode == CountriesDatasource}?.CountryId}

def CountryId= (GetCountryIdByCountryCode('CountriesDatasource'))

if ( CountryId == "" || CountryId== null ) 
    testRunner.gotoStepByName("AddCountry")
else{
	testRunner.gotoStepByName("UpdateCountry")
}
//process country(add/update) it does not need any Id to process</script>
