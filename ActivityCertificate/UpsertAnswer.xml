      <script>import static com.jayway.jsonpath.JsonPath.parse

//Get the parsed json of GetSources test step response
def json = new groovy.json.JsonSlurper().parseText(context.expand('${GetAnswer#Response}'))

//datasource  
def AnswerDescription =context.expand( '${#TestCase#AnswerDescription}')

//SourceId for a given SourceCode of the json
def getAnsIdByAnsDescr = { code ->  json.find {it?.SourceCode == AnswerDescription}?.AnswerId }


def AnswerId= (getAnsIdByAnsDescr( 'AnswerDescription'))

def response = context.expand( '${GetAnswer#Response}' )

if(response==null || response =="")
  	return QuestionId;

 else
 {
    AnswerId = context.expand( '${GetAnswer#Response#$[\'AnswerId\']}' )
    return AnswerId;
 }</script>
