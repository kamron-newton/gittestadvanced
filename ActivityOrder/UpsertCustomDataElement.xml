      <script>import static com.jayway.jsonpath.JsonPath.parse

//Get the parsed json of GetSources test step response
def json = new groovy.json.JsonSlurper().parseText(context.expand('${GetCustomDataElements#Response}'));

//datasource
def CustomDataNameDatasource =context.expand( '${RewardProviderCustomAttributes#CustomAttribute}');

//CustomDataId, CustomDataName, EntityName
def getCustomDataIdByCustomAttribute = { code ->  json.find {it?.CustomAttribute == CustomDataNameDatasource}?.CustomDataId }
def getCustomDataNameByCustomAttribute = { code ->  json.find {it?.CustomAttribute == CustomDataNameDatasource}?.CustomDataName }
def getCustomDataEntityNameByCustomAttribute = { code ->  json.find {it?.CustomAttribute == CustomDataNameDatasource}?.EntityName }
