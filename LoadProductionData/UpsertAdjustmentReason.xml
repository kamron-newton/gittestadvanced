    <con:settings/>
    <con:config>
      <script>import static com.jayway.jsonpath.JsonPath.parse

//Get the parsed json of GetSources test step response
def json = new groovy.json.JsonSlurper().parseText(context.expand('${GetAdjustmentReasons#Response}'))

//datasource
def adjReasonsDatasource =context.expand( '${AdjustmentReasons#AdjustmentReasonCode}')

//AdjustmentReasonId for a given AdjustmentReasonCode of the json
def getAdjReasonIdByAdjCode = { code ->  json.find {it?.AdjustmentReasonCode == adjReasonsDatasource}?.AdjustmentReasonId}

def adjustmentReasonId= (getAdjReasonIdByAdjCode( 'adjReasonsDatasource'))
 
if ( adjustmentReasonId == "" || adjustmentReasonId== null ) 
    testRunner.gotoStepByName("AddAdjustmentReason")
else{
	testRunner.gotoStepByName("AdjustmentReasonId")
  }</script>
