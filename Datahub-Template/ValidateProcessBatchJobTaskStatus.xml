    <con:settings/>
    <con:config>
      <script>Integer maxTime = 180
Integer elapsedTime = 0
Integer delay = 5
def batchjobretrieved = testRunner.testCase.getPropertyValue('BatchJobRetrieved')
log.info batchjobretrieved

//Loop until the job status is success
//Check the status every 5 seconds
while (elapsedTime &lt; maxTime){
if (batchjobretrieved != "True"){
def batchJobResponseStatus = testRunner.testCase.testSteps['GetBatchJobByExternalProcessId'].testRequest.response.statusCode
//log.info batchJobResponseStatus
if(batchJobResponseStatus == 200) {
testRunner.testCase.setPropertyValue('BatchJobRetrieved', 'True')
break
}
else if(batchJobResponseStatus != 404) {
testRunner.testCase.setPropertyValue('BatchJobRetrieved', 'True')
break
}
}



Thread.sleep(delay * 1000)
elapsedTime += delay
}</script>
    </con:config>
