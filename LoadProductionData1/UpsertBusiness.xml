def GetBusinessCode = { code ->  json.find {it?.BusinessCode == BusinessCodeDataSource}?.BusinessCode }

def BusinessCode= (GetBusinessCode('BusinessCodeDataSource')) 

if ( BusinessCode == "" || BusinessCode== null ) 
