          <con:configuration>
            <path>$.[?(@.TransactionId == '${PR Transaction#RawTransactionId}' &amp;&amp; @.MomentId == '${Upsert Fixed 250 Point Moment with 500 Cap#MomentId}')].PointsAmount[0]</path>
