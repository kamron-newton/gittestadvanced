def CustomDataName= (getCustomDataNameByCustomAttribute('CustomDataNameDatasource'))
def EntityName= (getCustomDataEntityNameByCustomAttribute('CustomDataNameDatasource'))


if (( CustomDataId == "" || CustomDataId== null ) &amp;&amp; (CustomDataName == "" || CustomDataName== null ) &amp;&amp; (EntityName == "" || EntityName== null ))
