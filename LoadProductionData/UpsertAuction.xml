  <con:testStep type="groovy" name="AuctionIDCond" id="f93eb4bb-4f5f-4098-b3b1-b1bfb0ac3e29">
    <con:settings/>
    <con:config>
      <script>import static com.jayway.jsonpath.JsonPath.parse
def auctionId = context.expand( '${GetAuctions#Response#$[0][\'AuctionId\']}' )
if (auctionId == "") {
	testRunner.gotoStepByName("AddAuction") 
}
def status = context.expand( '${GetAuctions#Response#$[0][\'Status\']}' )
if  (status !="" &amp;&amp; status != 'C')	
   testRunner.gotoStepByName("AuctionID")
if (status == 'C')
   testRunner.gotoStepByName("AuctionLoop")</script>
    </con:config>
